<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="VO.storageVO" table="storage">

		<id name="id" type="int" column="id">
			<generator class="increment" />
		</id>
		
		<property name="storagePlaneName">
			<column name="storagePlaneName" />
		</property>
		
		<property name="storageDatasize">
			<column name="storageDatasize" />
		</property>
		
		
		<property name="storagePrice">
			<column name="storagePrice" />
		</property>
		
		<property name="storageDescription">
			<column name="storagedescription" />
		</property>
		
	</class>
</hibernate-mapping>